 RowBox[{
  RowBox[{
   RowBox[{"weightedRandomTarget", "[", 
    RowBox[{"base_", ",", "list_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"diffs", ",", "total", ",", "weights"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"diffs", "=", 
       RowBox[{"Table", "[", 
