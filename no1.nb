Cell["Implementation of two-point crossover:", "CodeText",
 CellChangeTimes->{{3.9607343114184732`*^9, 3.960734315120695*^9}, {
   3.960807377865855*^9, 3.96080737813843*^9}, 3.960816243906579*^9, {
   3.9610333797453012`*^9, 
   3.9610333815981083`*^9}},ExpressionUUID->"274758bd-cfd7-4682-8cd2-\
4c1d4e01ca5e"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"lengthVaryTwoPointCrossover", "[", 
    RowBox[{"p1_", ",", "p2_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "len1", ",", "len2", ",", "minLen", ",", "pt1", ",", "pt2", ",", "c1", ",",
        "c2", ",", "child"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"len1", "=", 
       RowBox[{"StringLength", "[", "p1", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"len2", "=", 
       RowBox[{"StringLength", "[", "p2", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"minLen", "=", 
       RowBox[{"Min", "[", 
        RowBox[{"len1", ",", "len2"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"pt1", ",", "pt2"}], "}"}], "=", 
       RowBox[{"Sort", "@", 
        RowBox[{"RandomSample", "[", 
         RowBox[{
          RowBox[{"Range", "[", "minLen", "]"}], ",", "2"}], "]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"c1", "=", 
       RowBox[{"Characters", "[", "p1", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"c2", "=", 
       RowBox[{"Characters", "[", "p2", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"child", "=", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"Take", "[", 
          RowBox[{"c1", ",", 
           RowBox[{"pt1", "-", "1"}]}], "]"}], ",", 
         RowBox[{"Take", "[", 
          RowBox[{"c2", ",", 
           RowBox[{"{", 
            RowBox[{"pt1", ",", "pt2"}], "}"}]}], "]"}], ",", 
         RowBox[{"Drop", "[", 
          RowBox[{"c1", ",", "pt2"}], "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"Length", "[", "child", "]"}], ">", "\[IndentingNewLine]", 
         "MAXLENGTH"}], ",", "\[IndentingNewLine]", 
        RowBox[{"child", "=", 
         RowBox[{"Take", "[", 
          RowBox[{"child", ",", "MAXLENGTH"}], "]"}]}]}], 
       "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"StringJoin", "[", "child", "]"}]}]}], "]"}]}], ";"}]], "Input",\

 CellChangeTimes->{{3.960712391082511*^9, 3.960712391893394*^9}, {
