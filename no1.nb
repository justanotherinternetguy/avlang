               RowBox[{"All", ",", "2"}], "]"}], "]"}], "^", "3"}], ")"}], "/", 
           RowBox[{"Total", "[", 
            RowBox[{
             RowBox[{"diffs", "[", 
              RowBox[{"[", 
               RowBox[{"All", ",", "2"}], "]"}], "]"}], "^", "3"}], "]"}]}]}],
          ";", "\[IndentingNewLine]", 
         RowBox[{"RandomChoice", "[", 
          RowBox[{"weights", "->", 
