         RowBox[{"{", 
          RowBox[{"word", ",", "list"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"total", "=", 
       RowBox[{"Total", "[", 
        RowBox[{"diffs", "[", 
         RowBox[{"[", 
          RowBox[{"All", ",", "2"}], "]"}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"total", "==", "0"}], ",", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"RandomChoice", "[", "list", "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"weights", "=", 
