          RowBox[{"#", "->", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"freqTable", "[", "#", "]"}], ">", "0"}], ",", 
             RowBox[{"N", "[", 
              RowBox[{
               RowBox[{"freqTable", "[", "#", "]"}], "/", "total"}], "]"}], ",",
