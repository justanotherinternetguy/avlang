        RowBox[{
         RowBox[{"#", ">", "0"}], "&"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"total", "=", 
      RowBox[{"Total", "[", 
       RowBox[{"Values", "[", "nonZeroEntries", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"total", "==", "0"}], ",", "\[IndentingNewLine]", "freqTable", 
       ",", "\[IndentingNewLine]", 
       RowBox[{"Association", "[", 
        RowBox[{
