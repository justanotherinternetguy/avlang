Cell[BoxData[
 RowBox[{
  RowBox[{"plotFitnessHeatmap", "[", "allPossibleWords_List", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "fitnessValues", ",", "reshapedValues", ",", "size", ",", 
      "wordsMatrix"}], "}"}], ",", "\[IndentingNewLine]", "\t  ", 
    RowBox[{
     RowBox[{"fitnessValues", "=", 
      RowBox[{"fitnessFunction", " ", "/@", " ", "allPossibleWords"}]}], ";", 
     "\[IndentingNewLine]", "\t  ", 
     RowBox[{"size", "=", 
      RowBox[{"Ceiling", "[", 
       RowBox[{"Sqrt", "[", 
        RowBox[{"Length", "[", "fitnessValues", "]"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\t  ", 
     RowBox[{"reshapedValues", "=", 
      RowBox[{"PadRight", "[", 
       RowBox[{
        RowBox[{"Partition", "[", 
         RowBox[{"fitnessValues", ",", "size", ",", "size", ",", 
          RowBox[{"{", 
           RowBox[{"1", ",", "1"}], "}"}], ",", 
