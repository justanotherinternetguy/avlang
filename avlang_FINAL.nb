 RowBox[{
  RowBox[{"mostFreqChar", "=", 
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{"Function", "[", 
      RowBox[{"col", ",", 
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"counts", ",", "filtered"}], "}"}], ",", 
         RowBox[{
          RowBox[{"filtered", "=", 
           RowBox[{"DeleteCases", "[", 
            RowBox[{"col", ",", "\"\<-\>\""}], "]"}]}], ";", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"filtered", "===", 
             RowBox[{"{", "}"}]}], ",", "\"\<-\>\"", ",", 
            RowBox[{
             RowBox[{"counts", "=", 
              RowBox[{"Tally", "[", "filtered", "]"}]}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"First", "@", 
              RowBox[{"First", "@", 
               RowBox[{"SortBy", "[", 
                RowBox[{"counts", ",", 
                 RowBox[{
                  RowBox[{"-", 
                   RowBox[{"Last", "[", "#", "]"}]}], "&"}]}], "]"}]}]}]}]}], 
           "]"}]}]}], "]"}]}], "]"}], ",", "transposed"}], "]"}]}], 
  ";"}]], "Input",
